    "graceful-fs": "2",
    "fstream": "0",
    "minimatch": "0",
    "mkdirp": "0",
    "nopt": "2",
    "npmlog": "0",
    "osenv": "0",
    "request": "2",
    "rimraf": "2",
    "semver": "~2.1",
